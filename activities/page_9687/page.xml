    <content>&lt;p&gt;&lt;/p&gt;&lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US" style="font-size: 1.7rem;"&gt;&lt;p style="text-align: left;"&gt;&lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US" style="font-size: 1.7rem;"&gt;&lt;img src="@@PLUGINFILE@@/octae-icon-resources.png" alt="" width="140" height="136" role="presentation" class="img-responsive atto_image_button_middle"&gt;&lt;/span&gt;&lt;span style="font-size: 1.7rem;"&gt;​&lt;/span&gt;&lt;/p&gt;&lt;p style="text-align: left;"&gt;&lt;span style="font-size: 1.7rem;"&gt;&lt;span data-scheme-color="@333333,," data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;These are extension resources that you&amp;nbsp;can use to expand your knowledge. You do not need access&amp;nbsp;these additional&amp;nbsp;&lt;/span&gt;&lt;span data-scheme-color="@333333,," data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;resources, but they are available to extend your&amp;nbsp;learning.&amp;nbsp;&lt;/span&gt;&lt;br&gt;&lt;/span&gt;&lt;/p&gt;&lt;/span&gt;&lt;ul role="list"&gt;&lt;li data-aria-posinset="1" role="listitem" data-aria-level="1"&gt;&lt;p xml:lang="EN-US" lang="EN-US" paraid="0" paraeid="{a908f5f2-fbf8-47a6-898b-1d0a3a3b0392}{198}" data-ccp-props="{&amp;quot;335551550&amp;quot;:1,&amp;quot;335551620&amp;quot;:1,&amp;quot;335559683&amp;quot;:0,&amp;quot;335559685&amp;quot;:240,&amp;quot;335559731&amp;quot;:-230,&amp;quot;335559737&amp;quot;:0,&amp;quot;335562764&amp;quot;:2,&amp;quot;335562765&amp;quot;:1.14,&amp;quot;335562766&amp;quot;:4,&amp;quot;335562767&amp;quot;:0,&amp;quot;335562768&amp;quot;:4,&amp;quot;335562769&amp;quot;:6}"&gt;&lt;a href="http://iris.peabody.vanderbilt.edu/_archive/iris-and-adult-learning-theory/" target="_blank" rel="noreferrer"&gt;&lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;How People Learn Framework and the STAR Legacy Model&amp;nbsp;&lt;/span&gt;&lt;/a&gt;&lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;of online adult learning from&amp;nbsp;&lt;/span&gt;&lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;Vanderbilt University's IRIS Center&lt;/span&gt;​&lt;/p&gt;&lt;/li&gt;&lt;li data-aria-posinset="2" role="listitem" data-aria-level="1"&gt;&lt;p xml:lang="EN-US" lang="EN-US" paraid="0" paraeid="{a908f5f2-fbf8-47a6-898b-1d0a3a3b0392}{201}" data-ccp-props="{&amp;quot;335551550&amp;quot;:1,&amp;quot;335551620&amp;quot;:1,&amp;quot;335559683&amp;quot;:0,&amp;quot;335559685&amp;quot;:240,&amp;quot;335559731&amp;quot;:-230,&amp;quot;335559737&amp;quot;:0,&amp;quot;335562764&amp;quot;:2,&amp;quot;335562765&amp;quot;:1.14,&amp;quot;335562766&amp;quot;:4,&amp;quot;335562767&amp;quot;:0,&amp;quot;335562768&amp;quot;:4,&amp;quot;335562769&amp;quot;:6}"&gt;&lt;span data-contrast="auto"&gt;&lt;/span&gt;&lt;a href="https://tilthighered.com/tiltexamplesandresources" target="_blank" rel="noreferrer"&gt;&lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;Transparency in Teaching and Learning Project&lt;/span&gt;&lt;/a&gt;&lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;&amp;nbsp;:TILT Higher Ed&lt;/span&gt;​&lt;/p&gt;&lt;/li&gt;&lt;li data-aria-posinset="3" role="listitem" data-aria-level="1"&gt;&lt;p xml:lang="EN-US" lang="EN-US" paraid="0" paraeid="{a908f5f2-fbf8-47a6-898b-1d0a3a3b0392}{204}" data-ccp-props="{&amp;quot;335551550&amp;quot;:1,&amp;quot;335551620&amp;quot;:1,&amp;quot;335559683&amp;quot;:0,&amp;quot;335559685&amp;quot;:240,&amp;quot;335559731&amp;quot;:-230,&amp;quot;335559737&amp;quot;:0,&amp;quot;335562764&amp;quot;:2,&amp;quot;335562765&amp;quot;:1.14,&amp;quot;335562766&amp;quot;:4,&amp;quot;335562767&amp;quot;:0,&amp;quot;335562768&amp;quot;:4,&amp;quot;335562769&amp;quot;:6}"&gt;&lt;a href="https://digitalpromise.org/2017/03/13/ti