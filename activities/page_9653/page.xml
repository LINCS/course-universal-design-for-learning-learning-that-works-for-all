    <content>&lt;p dir="ltr" style="text-align: left;"&gt;&lt;/p&gt;&lt;div&gt;&lt;p xml:lang="EN-US" lang="EN-US" paraid="0" paraeid="{0e34ed5f-64cc-4269-9e00-d291bab35beb}{91}" data-ccp-props="{&amp;quot;335551550&amp;quot;:1,&amp;quot;335551620&amp;quot;:1,&amp;quot;335559683&amp;quot;:0,&amp;quot;335559685&amp;quot;:17,&amp;quot;335559731&amp;quot;:0,&amp;quot;335559737&amp;quot;:0,&amp;quot;335562764&amp;quot;:2,&amp;quot;335562765&amp;quot;:1.13999,&amp;quot;335562766&amp;quot;:4,&amp;quot;335562767&amp;quot;:0,&amp;quot;335562768&amp;quot;:4,&amp;quot;335562769&amp;quot;:0}"&gt;&lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;strong style="font-size: 1.7rem;"&gt;&lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;&lt;span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/strong&gt;&lt;/p&gt;&lt;p&gt;&lt;span style="font-size: 1.7rem;"&gt;&lt;span data-usefontface="true" data-contrast="none" xml:lang="EN-US" lang="EN-US" style="font-weight: bold;"&gt;Video Title:&amp;nbsp;&lt;/span&gt;&lt;span data-usefontface="true" data-contrast="none" xml:lang="EN-US" lang="EN-US" style=""&gt;Jason Marshall and&amp;nbsp;&lt;/span&gt;&lt;span data-contrast="auto"&gt;&lt;/span&gt;&lt;span data-usefontface="true" data-contrast="none" xml:lang="EN-US" lang="EN-US" style=""&gt;Variability in Adult Learners&lt;/span&gt;&lt;/span&gt;&lt;span data-usefontface="true" data-contrast="none" xml:lang="EN-US" lang="EN-US" style="font-size: 1.7rem;"&gt;&lt;/span&gt;&lt;span style="font-size: 1.7rem;"&gt;​&lt;/span&gt;&lt;br&gt;&lt;/p&gt;&lt;div&gt;&lt;p xml:lang="EN-US" lang="EN-US" paraid="0" paraeid="{0e34ed5f-64cc-4269-9e00-d291bab35beb}{122}" data-ccp-props="{&amp;quot;335551550&amp;quot;:1,&amp;quot;335551620&amp;quot;:1,&amp;quot;335559683&amp;quot;:0,&amp;quot;335559685&amp;quot;:0,&amp;quot;335559731&amp;quot;:0,&amp;quot;335559737&amp;quot;:0,&amp;quot;335562764&amp;quot;:2,&amp;quot;33