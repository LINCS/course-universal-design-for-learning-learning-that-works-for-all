    <intro>&lt;p dir="ltr" style="text-align: left;"&gt;&lt;br&gt;&lt;/p&gt;&lt;p dir="ltr" style="text-align: left;"&gt;&lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;&lt;a data-sessionid="ea182251-f0b5-5e6e-c945-171f5e2b0cc7" data-shapeids="2,11" data-slide=""&gt;&amp;nbsp;&lt;/a&gt;&lt;br&gt;&lt;/span&gt;&lt;/p&gt;&lt;p dir="ltr" style="text-align: left;"&gt;&lt;span data-usefontface="false" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;&lt;a data-sessionid="ea182251-f0b5-5e6e-c945-171f5e2b0cc7" data-shapeids="2,11" data-slide=""&gt;&lt;br&gt;&lt;/a&gt;&lt;/span&gt;&lt;/p&gt;</intro>
